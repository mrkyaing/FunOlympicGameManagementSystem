@model IEnumerable<ContactEnquiryModel>
@{
    Layout = "Admin/_AdminLayout";
}
<div class="wrapper">
    <h1>Contact Enquiry Information</h1>
        <table id="ContactEnquiryTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Message</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model) {
                    <tr scope="row">
                        <td>@item.Name</td>
                        <td>@item.Email</td>
                        <td>@item.Phone</td>
                        <td>@item.Message</td>                    
                    </tr>
                }
            </tbody>
        </table>
</div>